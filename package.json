{
  "name": "tn93",
  "version": "0.2.1",
  "description": "A Javascript implementation of the TN93 genetic distance algorithm",
  "main": "dist/tn93.js",
  "directories": {
    "test": "test",
    "docs": "docs",
    "demo": "demo"
  },
  "scripts": {
    "docs": "node_modules/docco/bin/docco dist/tn93.js && mv docs/tn93.html docs/index.html",
    "minify": "node_modules/uglify-js/bin/uglifyjs --compress --mangle -- dist/tn93.js > dist/tn93.min.js",
    "start": "node_modules/http-server/bin/http-server ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AABoyles/tn93.js.git"
  },
  "author": "Tony Boyles",
  "license": "Apache-2.0",
  "devDependencies": {
    "docco": "^0.7.0",
    "http-server": "^0.11.1",
    "uglify-js": "^3.0.8"
  }
}
