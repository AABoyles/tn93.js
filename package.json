{
  "name": "tn93",
  "version": "0.3.0",
  "description": "A Javascript implementation of the TN93 genetic distance algorithm",
  "main": "dist/tn93.js",
  "directories": {
    "test": "test",
    "docs": "docs",
    "demo": "demo"
  },
  "scripts": {
    "docs": "docco dist/tn93.js && mv docs/tn93.html docs/index.html",
    "minify": "terser --compress --mangle -- dist/tn93.js > dist/tn93.min.js",
    "start": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AABoyles/tn93.js.git"
  },
  "author": "Tony Boyles",
  "license": "Apache-2.0",
  "devDependencies": {
    "docco": "^0.7.0",
    "http-server": "^0.12.1",
    "terser": "^4.2.0"
  }
}
